uint16 detection_id                   #the ID of this detection generated by radar

# The general info of the object
geometry_msgs/Point position          #only the x &y components are valid
geometry_msgs/Vector3 velocity        
uint8 dyn_prop                        #Dynamic property of the object indicating
float32 rcs                           #the Radar cross section

# The quality of the object
uint8 dislong_rms                     #Standard deviation of longitudinal distance
uint8 dislat_rms                      #Standard deviation of latitudinal distance
uint8 verllong_rms                    #Standard deviation of longitudinal relative velocity
uint8 verllat_rms                     #Standard deviation of latitudinal relative velocity
uint8 arellong_rms                    #Standard deviation of longitudinal relative acceleration
uint8 arellat_rms                     #Standard deviation of latitudinal relative acceleration
uint8 orientation_rms                 #Standard deviation of orientation angle
uint8 meas_state                      #Measurement state indicating Deleted/Measured/Predicted/from merge...
uint8 probofexist                     #Probability of existence

# The extended info of the object
float32 are_long                      #Relative acceleration in longitudinal direction
uint8 obj_class                       #point/car/truck/not in use/motorcycle/bicycle/wide/reserved
float32 are_lat                       #Relative acceleration in lateral direction (y)[permanently set to “Zero”]
float32 orientation_angle             #Orientation angle of the object
float32 length                        #Length of the tracked object
float32 width                         #Width of the tracked object